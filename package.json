{
  "name": "zthall-www",
  "private": true,
  "description": "Zack Hall portfolio site.",
  "version": "0.1.0",
  "author": "Zack Hall <zack@zthall.com>",
  "dependencies": {
    "@manishrc/tailwindcss-typography-js": "^0.1.4",
    "autoprefixer": "^10.4.20",
    "prismjs": "^1.19.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "gh-pages": "^2.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "postcss-preset-env": "^10.1.3",
    "prettier": "2.0.4",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.15"
  },
  "homepage": "https://zthall.com",
  "keywords": [
    "zthall",
    "Zack Hall"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zackhall/zthall-www.git"
  },
  "scripts": {
    "build:css": "postcss src/styles/styles.css -o _site/src/styles/styles.css",
    "build:11ty": "eleventy",
    "build": "npm run build:11ty && npm run build:css",
    "start": "npm run dev",
    "dev:css": "postcss src/styles/styles.css -o _site/src/styles/styles.css --watch",
    "dev:11ty": "eleventy --serve",
    "dev": "npm-run-all --parallel dev:*",
    "deploy": "npm run build && node deploy.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "clean": "rm -rf _site"
  }
}
